{
  "name": "obsidian-textlint",
  "version": "2.1.1",
  "description": "A plugin for Obsidian that integrates textlint",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "setup": "npm run setup-textlint && npm run build",
    "setup-textlint": "npm --prefix textlint-builder install && npm --prefix textlint-builder run build",
    "version": "node version-bump.mjs",
    "release": "cat package.json | jq '.version' | xargs ./scripts/release.sh"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/lint": "^6.1.0",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.7.3",
    "@textlint/script-compiler": "^0.15.0",
    "@textlint/types": "^13.1.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.6",
    "lodash-es": "^4.17.21"
  }
}
